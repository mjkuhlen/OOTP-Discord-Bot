generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model divisions {
  league_id     Int
  sub_league_id Int
  division_id   Int
  name          String? @db.VarChar(50)
  gender        Int?

  @@id([league_id, sub_league_id, division_id])
}

model games {
  game_id         Int       @id
  league_id       Int?
  home_team       Int?
  away_team       Int?
  attendance      Int?
  date            DateTime? @db.Date
  time            Int?      @db.SmallInt
  game_type       Int?      @db.SmallInt
  played          Int?      @db.TinyInt
  innings         Int?      @db.SmallInt
  runs0           Int?      @db.SmallInt
  runs1           Int?      @db.SmallInt
  hits0           Int?      @db.SmallInt
  hits1           Int?      @db.SmallInt
  errors0         Int?      @db.SmallInt
  errors1         Int?      @db.SmallInt
  winning_pitcher Int?
  losing_pitcher  Int?
  save_pitcher    Int?
  starter0        Int?
  starter1        Int?
  homeTeam        teams?   @relation("HomeTeam", fields: [home_team], references: [team_id])
  awayTeam        teams?   @relation("AwayTeam", fields: [away_team], references: [team_id])
  winningPitcher  players? @relation("WinningPitcher", fields: [winning_pitcher], references: [player_id])
  losingPitcher   players? @relation("LosingPitcher", fields: [losing_pitcher], references: [player_id])
  savePitcher     players? @relation("SavePitcher", fields: [save_pitcher], references: [player_id])
  starter0Player  players? @relation("Starter0", fields: [starter0], references: [player_id])
  starter1Player  players? @relation("Starter1", fields: [starter1], references: [player_id])
}

